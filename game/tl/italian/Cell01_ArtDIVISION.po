# game/Cell01_ArtDIVISION.rpy:1471 #-#-# translate crowdin Quest_A_Item_Luggage03_End_3c6d1dc6:
msgid "[_a_] If you want, I can give you some. "
msgstr "[_a_] Se vuoi, io posso darti un po'. "

# game/Cell01_ArtDIVISION.rpy:1473 #-#-# translate crowdin Quest_A_Item_Luggage03_End_45f1a88e:
